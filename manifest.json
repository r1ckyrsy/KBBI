{
	"name": "KBBI (Anywhere)",
	"short_name": "KBBI",
	"description": "A web extension to search for the meaning of a word, with access floating on any web page",
	"author": "Ricky R. Syahputra",
	"version": "0.1.0",
	"manifest_version": 2,
	"permissions": [
  ],
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["lib/js/r.js", "inject.js"],
			"css": ["lib/css/inject.css"],
			"run_at": "document_start",
			"all_frames": true
		},
		{
			"matches": ["*://kbbi.kemdikbud.go.id/*"],
			"js": ["panel.js"],
			"run_at": "document_start",
			"all_frames": true
		}
	],
	"web_accessible_resources": [
		"lib/img/search.png", "preloader.html"
	]
}